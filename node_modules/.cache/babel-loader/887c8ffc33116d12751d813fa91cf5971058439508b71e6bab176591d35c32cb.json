atan,\r\n              kesan: this.kesan,\r\n              quotes: this.quotes,\r\n              jabatan: this.jabatan\r\n          }, { merge: true });\r\n\r\n          const storageRef = ref(storage, 'prestasi/'+x);\r\n            uploadBytes(storageRef, this.$refs.alumnibaru.files[0]).then(()=>{console.log(\"uploaded\")})\r\n        \r\n            this.nama= \"\",\r\n            this.panggilan= '',\r\n            this.angkatan= ''\r\n            this.kesan= ''\r\n            this.quotes= ''\r\n            this.jabatan= ''\r\n            this.$refs.alumnibaru.value = null;\r\n\r\n            alert('new alumni uploaded')\r\n      },\r\n    }\r\n  }\r\n  </script>"],"mappings":"AA8EE,SAASA,GAAG,EAAEC,MAAM,EAAEC,MAAK,QAAS,oBAAmB;AACvD,OAAOC,EAAC,MAAO,0BAAyB;AACxC,SAAQC,OAAO,QAAO,0BAAyB;AAC/C,SAASC,GAAG,EAAEC,WAAW,QAAO,kBAAiB;AAEjD,eAAc;EACZC,IAAI,EAAC,UAAU;EACfC,UAAU,EAAC,CAEX,CAAC;EACDC,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,YAAY,EAAE,EAAE;MAChBC,IAAI,EAAE,EAAE;MACRC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,EAAE;MACVC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,SAAS,CAAC;EACjB,CAAC;EAEDC,OAAO,EAAC;IACN,MAAMD,SAASA,CAAA,EAAG;MACd,MAAME,OAAM,GAAI,MAAMnB,MAAM,CAACD,GAAG,CAACG,EAAE,EAAE,QAAQ,EAAE,QAAQ,CAAC;MACxD,IAAIiB,OAAO,CAACC,MAAM,CAAC,CAAC,EAAE;QAClB,IAAI,CAACX,YAAW,GAAIU,OAAO,CAACX,IAAI,CAAC,CAAC,CAACa,MAAM;MAC/C;IACF,CAAC;IACD,MAAMC,SAASA,CAAA,EAAG;MACd;MACA,MAAMrB,MAAM,CAACF,GAAG,CAACG,EAAE,EAAE,QAAQ,EAAE,QAAQ,CAAC,EAAE;QACtCmB,MAAM,EAAE,IAAI,CAACZ,YAAW,GAAI;MAChC,CAAC,EAAE;QAAEc,KAAK,EAAE;MAAK,CAAC,CAAC;MAEnB,IAAIC,CAAA,GAAI,CAAC,IAAI,CAACf,YAAY,GAAC,CAAC,EAAEgB,QAAQ,CAAC,CAAC;MACxC,MAAMxB,MAAM,CAACF,GAAG,CAACG,EAAE,EAAE,QAAQ,EAAEsB,CAAC,CAAC,EAAE;QAC/Bd,IAAI,EAAE,IAAI,CAACA,IAAI;QACfC,SAAS,EAAE,IAAI,CAACA,SAAS;QACzBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,MAAM,EAAE,IAAI,CAACA,MAAM;QACnBC,OAAO,EAAE,IAAI,CAACA;MAClB,CAAC,EAAE;QAAEQ,KAAK,EAAE;MAAK,CAAC,CAAC;MAEnB,MAAMG,UAAS,GAAItB,GAAG,C