ale(0.85) ;\r\n    box-shadow: 0px 4px 4px 0px rgba(0, 0, 0, 0.25);\r\n    color: yellow;\r\n    border-radius: 15px;\r\n}\r\n\r\n.carousel__slide >  div > div > span{\r\n    color: white;\r\n}\r\n\r\n.carousel__slide--prev {\r\n  opacity: 1;\r\n  background-color: #001E6C;\r\n}\r\n\r\n.carousel__slide--next {\r\n  opacity: 1;\r\n  background-color: #001E6C;\r\n}\r\n\r\n.carousel__slide--active {\r\n  opacity: 1;\r\n  background-color: white;\r\n  color:#001E6C ;\r\n}\r\n\r\n.carousel__slide--active >  div > div > span{\r\n    color: #001E6C ;\r\n}\r\n\r\n.carousel__slide--active > div > span > img {\r\n  border-color: black\r\n}\r\n</style>"],"mappings":"AA2CA,SAASA,QAAQ,EAAEC,KAAK,EAAEC,UAAS,QAAS,eAAc;AAC1D,OAAO,iCAAgC;AACvC,SAASC,UAAS,QAAS,oBAAmB;AAC9C,OAAOC,EAAC,MAAO,0BAAyB;AACxC,SAASC,eAAc,QAAS,KAAI;AACpC,SAASC,aAAY,QAAS,SAAQ;AACtC;AACA;;AAEA,eAAeD,eAAe,CAAC;EAC3BE,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACZR,QAAQ;IACRC,KAAK;IACLC;EACA,CAAC;EACDO,KAAKA,CAAA,EAAE;IACL,MAAMC,KAAI,GAAIJ,aAAa,CAACH,UAAU,CAACC,EAAE,EAAE,QAAQ,CAAC,CAAC;IACrD,OAAM;MAACM;IAAK;EACd,CAAC;EACDC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,YAAY,EAAE,CAAC;MACfC,MAAM,EAAE;IAEZ,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAE,CAET,CAAC;EACDC,OAAO,EAAE;IACLC,MAAMA,CAACC,CAAC,EAAC;MACL,OAAO,uFAAsF,GAAGA,CAAC,GAAE,uDAAsD;IAC7J,CAAC;IACDC,OAAOA,CAACC,GAAG,EAAE;MACT,IAAI,CAACP,YAAW,GAAIO,GAAE;IAC1B;EACJ;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}